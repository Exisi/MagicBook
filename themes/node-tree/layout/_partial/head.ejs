<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">

	<!-- title -->
	<%
		let title = page.title;
		if (title == ''){
			title = page.slug.split("/")[page.slug.split("/").length-1];
		}
	%>
	<title>
	<% if (title){ %>
		<%= title %>&nbsp;&nbsp;âœ¦&nbsp;
	<% } %>
	<%= config.title %>
	</title>

	<!-- keywords,description -->
	<% if (page.keywords){ %>
		<meta name="keywords" content="<%= page.keywords %>,<%= config.keywords %>" />
	<% } %>

	<% if (config.keywords){ %>
		<meta name="keywords" content="<%= config.keywords %>" />
	<% } %>

	<% if (page.description){ %>
		<meta name="description" content="<%= page.description %>" />
	<% } %>

	<% if (config.description){ %>
		<meta name="description" content="<%= config.description %>" />
	<% } %>

	<!-- favicon -->
	<% if (theme.favicon){ %>
	<%- favicon_tag(theme.favicon) %>
	<% } %>


	<!-- search -->
	<script>
		let searchEngine = "<%- theme.searchEngine %>";
		if(typeof searchEngine == "undefined" || searchEngine == null || searchEngine == ""){
			searchEngine = "https://www.google.com/search?q=";
		}
		let homeHost = "<%- theme.homeHost %>";
		if(typeof homeHost == "undefined" || homeHost == null || homeHost == ""){
			homeHost = window.location.host;
		}
	</script>

	<%- css('css/main.css') %>
	<%- css('lib/font-awesome/font-awesome.min.css') %>
	<%- css('lib/darcula/darcula.min.css') %>
	<%- css('lib/jquery/jquery.fancybox.min.css') %>

	<%- js('lib/jquery/jquery-3.4.1.min.js') %>
	<%- js('lib/jquery/jquery.fancybox.min.js') %>
	<%- js('lib/jquery/jquery.pjax.min.js') %>
	<%- js('lib/highlight/highlight.min.js') %>
	<%- js('lib/darkreader/darkreader.min.js') %>
	<%- js('js/main.js') %>
	<% if (theme.busuanzi){ %>
		<script async src="//busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js"></script>
	<% } %>

	<% if (theme.baiduanalysis.enable){ %>
		<script>
				let _hmt = _hmt || [];
				(function () {
					let hm = document.createElement("script");
					hm.src = "<%- theme.baiduanalysis.src %>";
					let s = document.getElementsByTagName("script")[0];
					s.parentNode.insertBefore(hm, s);
				})();
		</script>
	<% } %>

	<% if (theme.googleanalysis.enable){ %>
		<script async src="<%- theme.googleanalysis.src %>"></script>
		<script>
			window.dataLayer = window.dataLayer || [];
			function gtag(){dataLayer.push(arguments);}
			gtag('js', new Date());
			gtag('config', 'G-M59MXF45E4');
		  </script>
	<% } %>
</head>